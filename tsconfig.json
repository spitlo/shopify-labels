{
  "compilerOptions": {
      "outDir": "./",
      "sourceMap": true,
      "strict": true,
      "noImplicitReturns": true,
      "noImplicitAny": true,
      "module": "es6",
      "moduleResolution": "node",
      "target": "es5",
      "allowJs": true,
  },
  "include": [
      "./src/**/*"
  ],
  "ignore": [
    "./assets"
  ]
}
