{
  "version": "0.3.0",
  "license": "MIT",
  "scripts": {
    "build": "parcel build src/index.html --out-dir ./assets --public-url assets/ --global shpf && mv assets/index.html .",
    "clean": "rm assets/*",
    "format": "eslint --quiet --fix 'src/ts/*.ts'; stylelint --fix 'src/scss/*.scss'",
    "lint": "eslint 'src/ts/*.ts'; stylelint 'src/scss/*.scss'",
    "start": "parcel src/index.html --global shpf"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "3.4.0",
    "@typescript-eslint/parser": "3.4.0",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "parcel-bundler": "1.12.4",
    "prettier": "2.0.5",
    "sass": "1.26.9",
    "stylelint": "13.6.1",
    "typescript": "3.9.5"
  },
  "dependencies": {
    "papaparse": "5.2.0"
  }
}
