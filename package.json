{
  "version": "0.6.1",
  "license": "MIT",
  "scripts": {
    "build": "parcel build src/index.html --out-dir ./assets --public-url assets/ --global shpf && mv assets/index.html .",
    "clean": "rm assets/*",
    "format": "eslint --quiet --fix 'src/ts/*.ts'; stylelint --fix 'src/scss/*.scss'",
    "lint": "eslint 'src/ts/*.ts'; stylelint 'src/scss/*.scss'",
    "start": "parcel src/index.html --global shpf"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "parcel-bundler": "1.12.4",
    "prettier": "2.1.2",
    "sass": "1.29.0",
    "stylelint": "13.7.2",
    "typescript": "4.0.5"
  },
  "dependencies": {
    "papaparse": "5.3.0"
  }
}
